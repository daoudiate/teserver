  "main": "index.js",
  "scripts": {
    "test": "mocha tests --recursive --watch",
    "start": "node index.js"
  },
  "author": {
    "name": "Youssef El",
    "email": "youssef@comepic.com",
    "url": "http://comepic.com"
  },
  "dependencies": {
