{
  "name": "teserver",
  "description": "A simple Docker server for extracting text from pdf, doc, docx, xls, xlsx, csv, pptx, png, jpg, gif, rtf...",
  "preferGlobal": true,
  "version": "0.0.5",
  "engines": {
    "node": ">=0.10"
  },
  "files": [
    "package.json",
    "index.js",
    "server/"
  ],
   "repository": {
    "type": "git",
    "url": "https://github.com/daoudiate/teserver.git"
  },
  "keywords": [
    "extraction",
    "text extraction",
    "doc",
    "docx",
    "pdf",
    "png",
    "jpg"
