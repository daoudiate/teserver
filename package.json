    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "lodash": "^4.14.0",
    "mime-types": "^2.1.13",
    "morgan": "^1.7.0",
    "multer": "^1.2.1",
    "multer-s3": "^2.5.0",
    "response-time": "^2.3.1",
    "textract": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
